doctype html
html(lang='en')
  include ../common/teacherHead.pug
  +head()
  body

    header
      h1 Gono University
        p Result Hub 
      include ../common/teacherNavbar.pug
      +navbar()
    h2 Chairmans
    table.assign
      tr
        th(scope="col") Session
        th(scope="col") Semester
        th(scope="col") email
        th(scope="col") Chairman
      for chairman in chairmans
        tr(id=chairman.id)
          td(scope="col") #{chairman.session_id}
          td(scope="col") #{chairman.semester}
          td(scope="col") #{chairman.email}
          td(scope="col") #{chairman.president}
          td 
            button(onclick=`deleteChairman('${chairman.id}')`) Delete    
  script(src="https://code.jquery.com/jquery-3.5.1.min.js")
  script(src="/js/deleteChairman.js")