doctype html
html(lang='en')
  include ../common/teacherHead.pug
  +head()
  body

    header
      h1 Gono University
        p Result Hub 
      include ../common/teacherNavbar.pug
      +navbar()
    h2 Created Sessions
    br
    table.assign
      tr
        th(scope="col") Session ID
        th(scope="col") Month
        th(scope="col") year
      for session in sessions
        tr(id=session.id)
          th(scope="col") #{session.session_id}
          th(scope="col") #{session.month}
          th(scope="col") #{session.year}
          th(scope="col")
            button(onclick=`DeleteSession('${session.id}')`) Delete    
  script(src="https://code.jquery.com/jquery-3.5.1.min.js")
  script(src="/js/DeleteSession.js")
